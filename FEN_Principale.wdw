      		IMG_OpenOffice..grisé = vrai
      	CAS cOpenOfficeWriter:
      		IMG_MicrosoftWord = IMG_MicrosoftWordGrise			
      		IMG_MicrosoftWord..Grisé=vrai
      FIN
      
