        code : |1-
         sablier(vrai)
         
         // Si un document est déjà ouvert
         si gpclEditeur:EstOuvert() ALORS
         	selon confirmer("Un document est déjà ouvert.","Voulez-vous le fermer ?")		
         		cas Oui
         			// Fermeture du document
         			gpclEditeur:FermerDocument()
         		cas Non
         			// Le document ouvert ne sera plus manipulable
         			info("Si vous réouvrez un document déjà actif, il sera en lecture seule")			
         		cas Annuler
         			sablier(faux)
         			retour
         	FIN
         FIN
         
         // Ouvrir un document
         si gpclEditeur:OuvrirDocument() = faux ALORS
         	erreur("L'ouverture du document a échoué.")
         sinon
         	fenetat(FEN_Principale,ActifDevant)
         FIN
         
         sablier(faux)
